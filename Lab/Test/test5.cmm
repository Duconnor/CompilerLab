int main()
{
    int a[10];
    int b[15];
    int i = 0, j = 0;
    while(i < 10)
    {
        a[i] = i * 2;
        i = i + 1;
    }
    while(j < 15)
    {
        b[j] = -j;
        j = j + 1;
    }
    i = 0;
    j = 0;
    while(i < 10)
    {
        write(a[i] * b[i]);
        i = i + 1;
    }
    while(j < 5)
    {
        write(b[j + 10] * j);
        j = j + 1;
    }
    return 0;
}
