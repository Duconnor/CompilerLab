FUNCTION fact :
PARAM v1
v2 := #1
v3 := #1
LABEL label1 :
IF v3 <= v1 GOTO label2
GOTO label3
LABEL label2 :
v2 := v2 * v3
v3 := v3 + #1
GOTO label1
LABEL label3 :
RETURN v2
FUNCTION main :
DEC v4 20
v5 := #0
v6 := #5
LABEL label4 :
IF v5 < v6 GOTO label5
GOTO label6
LABEL label5 :
t10 := v5 * #4
t8 := &v4 + t10
t11 := v5 + #2
*t8 := t11
t17 := v5 * #4
t16 := &v4 + t17
t15 := *t16
ARG t15
t14 := CALL fact
WRITE t14
